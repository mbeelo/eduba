  // The Novelists Path - Sample data for now
  { path: 'novelists', title: 'It Was the Best of Times', author: 'Charles Dickens', content: 'It was the best of times, it was the worst of times, it was the age of wisdom, it was the age of foolishness.', difficulty_order: 1 },
  { path: 'novelists', title: 'Call Me Ishmael', author: 'Herman Melville', content: 'Call me Ishmael. Some years ago—never mind how long precisely—having little or no money in my purse.', difficulty_order: 2 },
  { path: 'novelists', title: 'Pride and Prejudice', author: 'Jane Austen', content: 'It is a truth universally acknowledged, that a single man in possession of a good fortune, must be in want of a wife.', difficulty_order: 3 },
